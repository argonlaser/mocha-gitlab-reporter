{
  "name": "mocha-gitlab-reporter",
  "version": "1.0.0",
  "description": "Reports Mocha test results as gitlab issue.",
  "main": "index.js",
  "scripts": {
    "test": "source dev.env && mocha --reporter . tests/dummy.js && standard",
    "test-all-pass": "source dev.env && mocha --reporter . tests/dummy-all-pass.js && standard",
    "test-no-report": "source dev.env && mocha tests/dummy.js && standard",
    "codestyle-fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argonlaser/mocha-gitlab-reporter.git"
  },
  "keywords": [
    "mocha",
    "reporter",
    "gitlab",
    "issues"
  ],
  "author": "Venkata krishna Sundararajan <vkvenkat94@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/argonlaser/mocha-gitlab-reporter/issues"
  },
  "homepage": "https://github.com/argonlaser/mocha-gitlab-reporter#readme",
  "dependencies": {
    "gitlab-create-issue": "^1.0.1",
    "left-pad": "^1.1.3",
    "mocha": "^3.2.0",
    "superagent": "^3.5.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "standard": "^8.6.0"
  }
}
